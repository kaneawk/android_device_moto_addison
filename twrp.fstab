# mount point   fstype     device                       device2                  flags
/system         ext4       /dev/block/bootdevice/by-name/system
/system_image   emmc       /dev/block/bootdevice/by-name/system
/data           f2fs       /dev/block/bootdevice/by-name/userdata                flags=encryptable=/dev/block/bootdevice/by-name/metadata;length=-16384
/cache          ext4       /dev/block/bootdevice/by-name/cache
/recovery       emmc       /dev/block/bootdevice/by-name/recovery
/boot           emmc       /dev/block/bootdevice/by-name/boot                    flags=backup=1
/misc           emmc       /dev/block/bootdevice/by-name/misc
/firmware       vfat       /dev/block/bootdevice/by-name/modem                   flags=fsflags="ro,nosuid,nodev"
/persist        ext4       /dev/block/bootdevice/by-name/persist                 flags=fsflags="noatime,nosuid,nodev";mounttodecrypt;display="Persist";backup=1
/fsg            ext4       /dev/block/bootdevice/by-name/fsg                     flags=backup=1;display="Fsg"
/efs1           emmc       /dev/block/bootdevice/by-name/modemst1                flags=backup=1;display=EFS
/efs2           emmc       /dev/block/bootdevice/by-name/modemst2                flags=backup=1;subpartitionof=/efs1
#/oem           ext4       /dev/block/bootdevice/by-name/oem
/dsp            ext4       /dev/block/bootdevice/by-name/dsp                     flags=backup=1;display="Dsp"
/external_sd    auto       /dev/block/mmcblk1p1    /dev/block/mmcblk1            flags=display="Micro SDcard";storage;wipeingui;removable
/usb-otg        auto       /dev/block/sda1        /dev/block/sda                 flags=display="USB OTG";storage;wipeingui;removable
